{
    "env": {
        "LEGATO_ROOT": "@{DIR}",
        "LEGATO_TARGET": "${LEGATO_TARGET:-%TARGET%}",
        "PATH": "$LEGATO_ROOT/bin:$PATH",
        "LEGATO_LANGUAGE_SERVER": "$LEGATO_ROOT/bin/languageServer/languageServer.js",
        "LEGATO_SNIPPETS": "$LEGATO_ROOT/framework/snippets/vscode:$LEGATO_ROOT/framework/tools/mkTools/snippets/vscode:${LEGATO_SNIPPETS}"
    },
    "install": [
    ],
    "info": {
        "name": "mangOH-%MANGOH_BOARD%-%TARGET%-legato",
        "version": "%VERSION%",
        "description": "Legato Framework built for mangOH %MANGOH_BOARD% and %TARGET_UPPERCASE% (%LEGATO_VERSION%)",
        "requires": [
            "swi-license_1.2",
            "swi-verify-license_latest",
            "swi-aptdeps-legato_1.0",
            "swi-verify-aptdeps_latest"
        ],
        "tags": [
            "mangOH",
            "%TARGET%"
        ],
        "leafMinVersion": "2.0.1"
    }
}
